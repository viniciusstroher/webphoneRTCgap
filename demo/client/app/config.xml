  -->
  
   <gap:plugin spec="https://github.com/apache/cordova-plugin-media#2.4.x" />
   <gap:plugin spec="https://github.com/apache/cordova-plugin-file" />
   <gap:plugin spec="https://github.com/apache/cordova-plugin-file-transfer" />
   <gap:plugin spec="https://github.com/phonegap-build/cordova-plugin-crosswalk-webview" />

   <!--<gap:plugin spec="https://github.com/viniciusstroher/sip-cordova" />

-->
 	<gap:plugin spec="https://github.com/alongubkin/phonertc" />
   <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <preference name="iosPersistentFileLocation" value="Library" />
    
