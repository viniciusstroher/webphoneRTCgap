   <gap:plugin spec="https://github.com/w3get/cordova-plugin-splashscreen" />
 
  <!-- plugin para ios usar wkwebview -->
   <gap:plugin spec="https://github.com/apache/cordova-plugin-wkwebview-engine" />
   
   <gap:plugin spec="https://github.com/w3get/PushPlugin#v2.5.0-pgb" />
   
   
   <feature name="CDVWKWebViewEngine">
      <param name="ios-package" value="CDVWKWebViewEngine" />
   </feature>

    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />

    <access origin="*" browserOnly="true" />

    <allow-navigation href="*://*.w3host.no-ip.org/*"/>
    <allow-navigation href="*://*.googleapis.com/*"/>
    <allow-navigation href="https://192.168.0.250/*" />
    <allow-navigation href="*://192.168.0.250/*"/>
    <allow-intent href="*://*.192.168.0.250/*" />

    <allow-intent href="rtsp://*/*" />
    
    <allow-intent href="geo:*" />

     <allow-intent href="rtsp://*/*" />
  
